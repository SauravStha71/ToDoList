{
  "expo": {
    "name": "My Todo App",
    "slug": "my-todo-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/checkmark.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/splashscreen_logo.png",
